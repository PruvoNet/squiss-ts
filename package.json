{
  "name": "squiss-ts",
  "version": "5.2.1",
  "description": "High-volume SQS poller",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^16 || ^18 || ^20"
  },
  "scripts": {
    "clean": "rm -rf .nyc_output coverage dist",
    "build": "yarn lint && yarn compile",
    "compile": "tsc",
    "test": "yarn lint && yarn mocha",
    "cover": "yarn clean && yarn nyc yarn mocha",
    "lint": "tslint -c tslint.json \"src/**/*.ts\" \"test/**/*.ts\"",
    "benchmark": "tsc --extendedDiagnostics --incremental false",
    "e2e:start": "docker compose -f e2e/docker-compose.yml up &",
    "e2e:stop": "docker compose -f e2e/docker-compose.yml down &"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PruvoNet/squiss-ts.git"
  },
  "files": [
    "/dist/*.d.ts",
    "/dist/*.js"
  ],
  "keywords": [
    "aws",
    "amazon",
    "sqs",
    "polling",
    "poll",
    "poller",
    "queue",
    "typescript",
    "sns",
    "s3",
    "extended",
    "client",
    "sdk"
  ],
  "author": "Regev Brody <regevbr@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/PruvoNet/squiss-ts/issues"
  },
  "homepage": "https://github.com/PruvoNet/squiss-ts#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.359.0",
    "@aws-sdk/client-sqs": "^3.359.0",
    "@aws-sdk/types": "^3.357.0",
    "linked-list": "2.1.0",
    "ts-type-guards": "^0.7.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^14.18.48",
    "@types/proxyquire": "^1.3.28",
    "@types/uuid": "^9.0.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "delay": "5.0.0",
    "dirty-chai": "^2.0.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^15.1.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.1.3"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "exclude": [
      "src/test/**",
      "e2e**"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  }
}
