{
  "name": "squiss-ts",
  "version": "4.0.9",
  "description": "High-volume SQS poller",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rm -rf node_modules build coverage dist",
    "build": "npm run lint && npm run buildDist",
    "buildDist": "rm -rf dist/* && ./node_modules/.bin/tsc",
    "test": "npm run lint && npm run mocha",
    "mocha": "node --preserve-symlinks -r source-map-support/register node_modules/mocha/bin/_mocha --opts src/test/mocha.opts",
    "cover": "rm -rf coverage && nyc --reporter=lcov --reporter=text-summary npm run test",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PruvoNet/squiss-ts.git"
  },
  "files": [
    "/dist/*.d.ts",
    "/dist/*.js"
  ],
  "keywords": [
    "aws",
    "amazon",
    "sqs",
    "polling",
    "poll",
    "poller",
    "queue",
    "typescript",
    "sns",
    "s3",
    "extended",
    "client",
    "sdk"
  ],
  "author": "Regev Brody <regevbr@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PruvoNet/squiss-ts/issues"
  },
  "homepage": "https://github.com/PruvoNet/squiss-ts#readme",
  "dependencies": {
    "aws-sdk": "^2.572.0",
    "iltorb": "2.4.5",
    "linked-list": "^2.1.0",
    "ts-type-guards": "^0.6.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.5",
    "@types/chai-as-promised": "^7.1.2",
    "@types/iltorb": "^2.3.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.8",
    "@types/proxyquire": "^1.3.28",
    "@types/uuid": "^3.4.6",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "delay": "^4.3.0",
    "dirty-chai": "^2.0.1",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  }
}
