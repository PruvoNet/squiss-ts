{
  "name": "squiss-ts",
  "version": "5.6.0",
  "description": "High-volume SQS poller",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^18 || ^20 || ^22 || ^24"
  },
  "scripts": {
    "clean": "rm -rf .nyc_output coverage dist",
    "build": "yarn lint && yarn compile",
    "compile": "tsc",
    "test": "yarn lint && yarn mocha",
    "ci": "yarn build && yarn cover",
    "cover": "yarn clean && yarn nyc yarn mocha",
    "lint": "eslint \"src/**/*.ts\"",
    "benchmark": "tsc --extendedDiagnostics --incremental false",
    "e2e:start": "docker compose -f e2e/docker-compose.yml up &",
    "e2e:stop": "docker compose -f e2e/docker-compose.yml down &"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PruvoNet/squiss-ts.git"
  },
  "files": [
    "/dist/*.d.ts",
    "/dist/*.js"
  ],
  "keywords": [
    "aws",
    "amazon",
    "sqs",
    "polling",
    "poll",
    "poller",
    "queue",
    "typescript",
    "sns",
    "s3",
    "extended",
    "client",
    "sdk"
  ],
  "author": "Regev Brody <regevbr@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/PruvoNet/squiss-ts/issues"
  },
  "homepage": "https://github.com/PruvoNet/squiss-ts#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.927.0",
    "@aws-sdk/client-sqs": "^3.927.0",
    "@aws-sdk/types": "^3.930.0",
    "linked-list": "2.1.0",
    "ts-type-guards": "^0.7.0",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/chai": "4.3.20",
    "@types/chai-as-promised": "7.1.8",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.0",
    "@types/proxyquire": "^1.3.31",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "chai": "4.3.10",
    "chai-as-promised": "7.1.1",
    "delay": "5.0.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "50.8.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "mocha": "^11.7.5",
    "nyc": "^17.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "17.0.1",
    "sinon-chai": "3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "exclude": [
      "src/test/**",
      "e2e**",
      "build**",
      "eslint.config.js"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  }
}
